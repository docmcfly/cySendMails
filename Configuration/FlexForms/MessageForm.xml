							<exclude>1</exclude>
							<label>LLL:EXT:cy_send_mails/Resources/Private/Language/locallang_be_messageForm.xlf:flexforms_setting.frontendUserStorageUids
							</label>
							<config>
								<type>select</type>
								<renderType>selectTree</renderType>
								<foreign_table>pages</foreign_table>
								<foreign_table_where>ORDER BY pages.sorting</foreign_table_where>
