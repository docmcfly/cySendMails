							<type>select</type>
							<renderType>selectTree</renderType>
							<foreign_table>pages</foreign_table>
							<foreign_table_where>ORDER BY pages.sorting</foreign_table_where>
							<size>20</size>
							<treeConfig>
								<parentField>pid</parentField>
							</treeConfig>
							<minitems>1</minitems>
							<maxitems>1</maxitems>
						</config>
					</settings.messagesStorageUid>
					<!-- end of settings -->

				</el>
			</ROOT>
