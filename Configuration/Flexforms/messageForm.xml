<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
	<sheets>
		<sOPT>
			<ROOT>
				<sheetTitle>LLL:EXT:cy_send_mails/Resources/Private/Language/locallang_be_messageForm.xlf:flexforms_sheet_title.common</sheetTitle>
				<type>array</type>
				<el>
					<settings.saveMessages>
						<exclude>1</exclude>
						<label>LLL:EXT:cy_send_mails/Resources/Private/Language/locallang_be_messageForm.xlf:flexforms_setting.saveMessages</label>
						<config>
							<type>select</type>
							<default>none</default>
							<renderType>selectSingle</renderType>
							<items>
								<numIndex index="0">
									<numIndex index="0">LLL:EXT:cy_send_mails/Resources/Private/Language/locallang_be_messageForm.xlf:flexforms_setting.saveMessages.none
									</numIndex>
									<numIndex index="1">none</numIndex>
								</numIndex>
								<numIndex index="1">
									<numIndex index="0">LLL:EXT:cy_send_mails/Resources/Private/Language/locallang_be_messageForm.xlf:flexforms_setting.saveMessages.minimal
									</numIndex>
									<numIndex index="1">minimal</numIndex>
								</numIndex>
								<numIndex index="2">
									<numIndex index="0">LLL:EXT:cy_send_mails/Resources/Private/Language/locallang_be_messageForm.xlf:flexforms_setting.saveMessages.full
									</numIndex>
									<numIndex index="1">full</numIndex>
								</numIndex>
							</items>
						</config>
					</settings.saveMessages>
					<settings.senderNameSuffix>
						<exclude>1</exclude>
						<label>LLL:EXT:cy_send_mails/Resources/Private/Language/locallang_be_messageForm.xlf:flexforms_setting.senderNameSuffix
						</label>
						<config>
							<type>input</type>
							<eval>trim</eval>
						</config>
					</settings.senderNameSuffix>
					<settings.noReplySenderEmail>
						<exclude>1</exclude>
						<label>LLL:EXT:cy_send_mails/Resources/Private/Language/locallang_be_messageForm.xlf:flexforms_setting.noReplySenderEmail
						</label>
						<config>
							<type>input</type>
							<eval>required, email</eval>
							<default>noreply@domain.tld</default>
						</config>
					</settings.noReplySenderEmail>
					<settings.subjectPrefix>
						<exclude>1</exclude>
						<label>LLL:EXT:cy_send_mails/Resources/Private/Language/locallang_be_messageForm.xlf:flexforms_setting.subjectPrefix
						</label>
						<config>
							<type>input</type>
						</config>
					</settings.subjectPrefix>
					<settings.maxAttachmentsSize>
						<exclude>1</exclude>
						<label>LLL:EXT:cy_send_mails/Resources/Private/Language/locallang_be_messageForm.xlf:flexforms_setting.maxAttachmentsSize
						</label>
						<config>
							<type>input</type>
							<eval>int</eval>
							<default>5242880</default>							<!-- 5MB -->
						</config>
					</settings.maxAttachmentsSize>
				</el>
			</ROOT>
		</sOPT>
		<sSTORAGES>
			<ROOT>
				<sheetTitle>LLL:EXT:cy_send_mails/Resources/Private/Language/locallang_be_messageForm.xlf:flexforms_sheet_title.storages</sheetTitle>
				<type>array</type>
